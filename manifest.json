{
  "manifest_version": 3,
  "name": "Manifest",
  "description": "This extension will provide you daily affirmations to help you manifest the best version of yourself",
  "version": "1.0",
  "browser_action": {
    "default_icon": "manifest.png",
    "default_popup": "index.html" 
  },
  "permissions": [
    "activeTab"
  ],

  "chrome_url_overrides": {
    "newtab": "myPage.html"
  },

  "content_script": [
    {
      "html": "index.html"
    }
  ]
}


